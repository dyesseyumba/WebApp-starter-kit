{
  "name": "webapp-starter-kit",
  "version": "0.3.0",
  "description": "Starter kit for web front-end projects (React, Redux, Typescript).",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel lint open:src",
    "open:src": "node buildScripts/srcServer.js",
    "lint": "esw --ext .ts, .tsx webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist",
    "clean-coverage": "rimraf ./coverage",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "node buildScripts/build.js",
    "postbuild": "node buildScripts/distServer.js"
  },
  "author": "Hse Dev Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "material-ui": "^0.19.4",
    "prop-types": "^15.6.0",
    "react": "16.1.1",
    "react-css-modules": "^4.7.1",
    "react-dom": "16.1.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-connect": "^5.1.0",
    "redux-form": "^7.1.2",
    "redux-form-material-ui": "^4.3.0",
    "redux-saga": "^0.16.0",
    "typescript-fsa": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/classnames": "^0.0.32",
    "@types/jest": "^19.2.3",
    "@types/node": "^8.0.51",
    "@types/react": "16.0.22",
    "@types/react-addons-test-utils": "0.14.16",
    "@types/react-helmet": "5.0.3",
    "@types/react-intl": "2.3.2",
    "@types/react-redux": "5.0.12",
    "@types/react-router": "3.0.2",
    "@types/react-router-redux": "4.0.46",
    "@types/react-test-renderer": "16.0.0",
    "@types/redux": "^3.6.31",
    "@types/redux-devtools": "^3.0.37",
    "@types/redux-devtools-dock-monitor": "^1.1.30",
    "@types/redux-devtools-log-monitor": "^1.0.30",
    "@types/redux-form": "^7.0.9",
    "@types/redux-mock-store": "0.0.7",
    "@types/sinon": "^1.16.34",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.2",
    "@types/webpack-env": "^1.13.2",
    "awesome-typescript-loader": "^3.3.0",
    "chai": "^4.0.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.11.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^1.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "immutable": "^3.8.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "json-schema-faker": "^0.4.6",
    "json-server": "0.10.0",
    "localtunnel": "1.8.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
