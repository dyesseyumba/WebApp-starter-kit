    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "^21.2.2",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.6.1",
    "typescript-eslint-parser": "^9.0.0",
