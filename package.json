{
  "name": "webapp-starter-kit",
  "version": "0.4.0",
  "description": "Starter kit for web front-end projects (React, Redux, Typescript).",
  "scripts": {
    "prestart": "npm run lint && node buildScripts/startMessage.js",
    "start": "webpack-dev-server --config webpack.dev.js --mode development",
    "prelint": "esw --ext .js, webpack.config.* buildScripts --color",
    "lint": "tslint -t stylish src/**/*.ts{,x}",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist",
    "clean-coverage": "rimraf ./coverage",
    "clean-lib": "rimraf ./node_modules",
    "prebuild": "npm-run-all clean-dist clean-coverage lint",
    "build": "webpack --config webpack.prod.js --mode production",
    "postbuild": "node buildScripts/distServer.js",
    "stats": "webpack --config webpack.prod.js --mode production --json > stats.json",
    "prettier": "prettier --write \"{src,__{tests,mocks}__}/**/*.ts{,x}\"",
    "precommit": "lint-staged"
  },
  "author": "Hse Dev Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-cropper": "^1.0.1",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-popper": "^0.8.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.3.0",
    "redux-oidc": "^3.0.0-beta.16",
    "redux-saga": "^0.16.0",
    "remove": "^0.1.5",
    "simple-line-icons": "^2.4.1",
    "sweetalert": "2.0.6",
    "toastr": "^2.1.4",
    "typescript-fsa": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/classnames": "^2.2.3",
    "@types/faker": "^4.1.2",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "@types/react": "16.0.40",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-cropper": "^0.10.2",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-helmet": "5.0.5",
    "@types/react-intl": "2.3.5",
    "@types/react-redux": "5.0.15",
    "@types/react-router": "4.0.22",
    "@types/react-router-dom": "4.2.4",
    "@types/react-router-redux": "5.0.12",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-transition-group": "^2.0.7",
    "@types/redux-form": "^7.2.0",
    "@types/redux-mock-store": "^0.0.13",
    "@types/seamless-immutable": "^7.1.1",
    "@types/toastr": "^2.1.35",
    "@types/webpack": "^3.8.8",
    "@types/webpack-env": "^1.13.5",
    "add": "^2.0.6",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cache-loader": "^1.2.2",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.0",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.1.0",
    "immutable": "^3.8.2",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-testkit": "^1.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "seamless-immutable": "^7.1.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "thread-loader": "^1.1.5",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.1",
    "webpack-merge": "^4.1.2",
    "webpack-simple-progress-plugin": "^0.0.4",
    "yarn": "^1.5.1"
  },
  "lint-staged": {
    "*.ts{,x}": [
      "prettier --write \"{src,__{tests,mocks}__}/**/*.ts{,x}\"",
      "git add",
      "tslint -t stylish \"src/**/*.ts{,x}\""
    ],
    "*.js": [
      "prettier --write",
      "git add",
      "esw --ext .js, webpack.config.* buildScripts --color"
    ]
  }
}
